{
  "devDependencies": {
    "uuid": "^9.0.0",
    "@babel/cli": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@faker-js/faker": "^8.0.2",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@wix/cli": "^1.0.0",
    "@wix/eslint-plugin-cli": "^1.0.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^4.1.0",
    "husky": "^8.0.3",
    "jest": "^29.1.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "prettier-eslint": "^15.0.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.5"
  },
  "scripts": {
    "build": "tsc --build app && node ./scripts/rename.js && node ./scripts/modifyAPI.js && eslint --quiet .",
    "postinstall": "wix sync-types",
    "dev": "wix dev",
    "lint": "eslint . --fix",
    "test": "jest",
    "prepare": "husky install",
    "watch-src": "nodemon --watch 'app/**/*' --ext ts --exec 'npm run build'",
    "watch-tests": "nodemon --watch 'tests/**/*' --ext ts --exec 'npm run test'",
    "watch": "npm run watch-src & npm run watch-tests",
    "preview": "npm run build && URL=$(wix preview --source local | grep -o 'http://wix.to/[^ ]*') && sleep 2 && echo $URL && echo $URL | xargs open"
  }
}